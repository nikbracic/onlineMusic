<div class="head">
  <h1>Songs</h1>
</div>
<table>
  <tr>
    <th>Uploaded Files</th>
  </tr>
  <% for asset in @assets %>
      <tr>
        <td><%= link_to asset.file_name, download_url(asset)%></td>
        <td><%= link_to "Edit", edit_asset_path(asset) %></td>
        <td><%= link_to "Delete", asset, :confirm => 'Are you sure?', :method => :delete %></td>
      </tr>
  <% end %>
</table>
<div class="links">
  <p><%= link_to "New Song", new_asset_path %></p>
</div>


